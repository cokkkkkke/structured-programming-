#include<stdio.h>

void multiplyArray(int *arr, int n, int factor){
    for(int i=0;i<n;i++){
        *(arr+i) = *(arr+i) * factor;
    }
}

int main(){
    int n, factor;
    printf("Enter number of elements: ");
    scanf("%d",&n);
    int arr[n];

    printf("Enter array elements:\n");
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);

    printf("Enter multiplication factor: ");
    scanf("%d",&factor);

    multiplyArray(arr, n, factor);

    printf("Array after multiplication:\n");
    for(int i=0;i<n;i++)
        printf("%d ", arr[i]);
    printf("\n");

    return 0;
}
